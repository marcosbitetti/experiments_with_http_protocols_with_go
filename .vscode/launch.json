    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch HTTP Server",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd",
            "env": {},
            "args": []
        },
        {
            "name": "Test Current File",
            "type": "go",
            "request": "launch",
            "mode": "test",
            "program": "${file}",
            "env": {},
            "args": []
        },
        {
            "name": "Run Benchmarks",
            "type": "go",
            "request": "launch",
            "mode": "test",
            "program": "${workspaceFolder}/cmd",
            "env": {},
            "args": ["-bench=.", "-benchmem"]
        },
        {
            "name": "Debug HTTP3 Tests",
            "type": "go",
            "request": "launch",
            "mode": "test",
            "program": "${workspaceFolder}/cmd",
            "env": {},
            "args": ["-run=BenchmarkHTTP3"]
        }
    ]
}
